{
  "name": "user-service",
  "description": "User service for TKO-äly",
  "authors": "TKO-äly ry",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/TKOaly/user-service#tko-%C3%A4ly-user-service",
  "bugs": "https://github.com/TKOaly/user-service/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/TKOaly/user-service"
  },
  "scripts": {
    "start": "node dist/src/index.js",
    "watch": "ts-node-dev src/index.ts",
    "test": "nyc mocha",
    "create-uuid": "ts-node scripts/create_uuid.ts",
    "generate-doc": "typedoc --ignoreCompilerErrors --out ./doc/ src/**/*.ts",
    "db:init": "ts-node scripts/create_databases.ts",
    "ci:db:init": "node ./dist/scripts/create_databases.ts",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "fix-ts": "eslint '*/**/*.{ts,tsx}' --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -p tsconfig.json",
    "build-docker": "docker build . -t user-service",
    "run-docker": "docker run -p 8080:8080 --env-file=.env -d --name user-service-container user-service"
  },
  "dependencies": {
    "@types/lodash": "^4.14.162",
    "@types/morgan": "^1.9.1",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-mysql-session": "^2.1.4",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "i18n": "^0.13.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.6",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "node-sass-middleware": "^0.11.0",
    "pug": "^3.0.0",
    "raven": "^2.6.4",
    "secure-compare": "^3.0.1",
    "sha1": "^1.1.1",
    "typedoc": "^0.19.2",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/chai-http": "^4.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.9.36",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-mysql-session": "^2.1.1",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "^0.0.48",
    "@types/i18n": "^0.8.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.3",
    "@types/mysql": "^2.15.15",
    "@types/mysql2": "https://github.com/types/mysql2.git",
    "@types/node": "^14.11.8",
    "@types/node-sass-middleware": "^0.0.31",
    "@types/raven": "^2.5.3",
    "@types/selenium-webdriver": "^4.0.9",
    "@types/sha1": "^1.1.2",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "uuid": "^8.3.1"
  },
  "engines": {
    "node": ">=10.8",
    "yarn": ">=1.6.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "scripts/**/*.ts",
      "src/controllers/Controller.ts",
      "migrations/**/*.js",
      "migrations/**/*.ts",
      "seeds/**/*.js",
      "seeds/**/*.ts",
      "coverage/**/*.js",
      "**/*.js",
      "knexfile.ts",
      "src/App.ts",
      "src/index.ts",
      "src/i18n.config.ts",
      "node_modules"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "all": true
  }
}
